{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "AuthorizedAccountEndpoint": {
    "BaseUri": "your uri <optional> if not extracting any value from any endpoints to populate accounts",
    "ApiKey": "",
    "EndpointRoute": "authorisation/organisation/{0}/accounts",
    "CustomClaimName": "AuthorisedUser"
  },
  "ValidatedJwtToken": false,
  "JwtTokenConfiguration": {
    "ValidateIssuer": false,
    "ValidateAudience": false,
    "ValidateLifetime": false,
    "ValidateIssuerSigningKey": false,
    "ValidIssuer": "",
    "ValidAudience": "",
    "IssuerSigningKey": ""
  },
  "AllowedHosts": "*"
}
